openapi: 3.0.0
info:
  version: 2.0.0
  title: Medius Supplier & Contracts Data Model
  license:
    name: MIT
servers:
  - url: https://{tenent}QA.medius-s2c.com/S2C/
    description: UAT server for testing
  - url: https://{tenent}.medius-s2c.com/S2C/
    description: PRODUCTION server
security:
  - ApiKeyAuth: []
paths:
  /S2CAPI/v1/suppliers:
    get:
      summary: Gets a list of basic supplier information
      parameters:
        - name: LastSupplierUpdate
          in: query
          required: false
          description: The date to filter the suppliers by the latest modified date in medius, using ISO 8601 format yyyy-MM-ddTHH:mm:ssZ (2021-20-10T14:01:54.9571247Z)
          example: '2008-09-22T14:01:54.9571247Z'
          schema:
            type: string
      operationId: GetSuppliers
      tags:
        - Suppliers
      responses:
        '200':
          description: List of basic supplier details
          content:
            application/xml:
              schema:
                $ref: '#/components/schemas/ArrayOfSupplier'
              examples:
                GetSuppliers:
                  value: |-
                    <ArrayOfSupplier xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                      <Supplier>
                          <Name>Sodales At Corp.</Name>
                          <Code>SUP001</Code>
                          <SupplierApprovalStatus>New</SupplierApprovalStatus>
                          <SupplierModifiedDate>05/05/2022 11:27:23</SupplierModifiedDate>
                      </Supplier>
                      <Supplier>
                          <Name>Nulla Magna Malesuada Company</Name>
                          <Code>SUP002</Code>
                          <SupplierApprovalStatus>New</SupplierApprovalStatus>
                          <SupplierModifiedDate>05/05/2022 11:27:23</SupplierModifiedDate>
                      </Supplier>
                        </ArrayOfSupplier>
            application/json:
              examples:
                GetSuppliers:
                  value:
                    Supplier:
                      - Name: Sodales At Corp.
                        Code: SUP001
                        SupplierApprovalStatus: New
                        SupplierModifiedDate: 05/05/2022 11:27:23
                      - Name: Nulla Magna Malesuada Company
                        Code: null
                        SupplierApprovalStatus: New
                        SupplierModifiedDate: 05/05/2022 11:27:23
