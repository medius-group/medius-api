  /S2CAPI/v1/contracts:
    description: Contract Management
    get:
      summary: Get all contracts
      description: Get all contracts filtered between the last modified date and the lastintegrated date. Also supports filtering on multiple contract statuses and types.
      parameters:
        - name: LastMediusUpdate
          in: query
          required: false
          description: The date to filter the last modified contracts by (Integrated date), using ISO 8601 format yyyy-MM-ddTHH:mm:ssZ (2021-20-10T14:01:54.9571247Z)
          example: '2008-09-22T14:01:54.9571247Z'
          schema:
            type: string
        - name: ContractStatus
          in: query
          required: false
          description: Filter the contracts by multiple contract statuses
          example: Effective,Expired
          schema:
            type: string
        - name: ContractType
          in: query
          required: false
          description: Filter the contracts by multiple contract types
          example: Framework,NDA
          schema:
            type: string
      operationId: GetContracts
      responses:
        default:
          description: Default error sample response
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Contract'
              examples:
                GetContracts:
                  value: |-
